<p id="notice"><%= notice %></p>

<h1>Cinemas</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Endereco</th>
      <th>Imagem</th>
      <th>Fachada</th>
      <th>Lat</th>
      <th>Long</th>
      <th>Cidades</th>
      <th>Descricao</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cinemas.each do |cinema| %>
      <tr>
        <td><%= cinema.nome %></td>
        <td><%= cinema.endereco %></td>
        <td><%= cinema.imagem %></td>
        <td><%= cinema.fachada %></td>
        <td><%= cinema.lat %></td>
        <td><%= cinema.long %></td>
        <td><%= cinema.cidades %></td>
        <td><%= cinema.descricao %></td>
        <td><%= link_to 'Show', cinema %></td>
        <td><%= link_to 'Edit', edit_cinema_path(cinema) %></td>
        <td><%= link_to 'Destroy', cinema, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cinema', new_cinema_path %>
